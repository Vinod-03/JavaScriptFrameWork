
let s="testYanta"
let  vow=0;
let cont=0;
            
   
        for(let i=0;i<s.length;i++)
        {
           
            if(s[i]=='a' || s[i]=='e'|| s[i]=='o' ||s[i]=='u'|| s[i]=='i'){
                vow++
              }
              else
              {
                cont++
              }

        }
        console.log(vow)
        console.log(cont)